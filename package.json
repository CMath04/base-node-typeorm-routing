{
  "name": "base-node-typeorm-routing",
  "version": "0.0.0",
  "description": "",
  "main": "dist/build/server.js",
  "scripts": {
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "test": "cross-env NODE_ENV=test jest --env=node --colors",
    "build": "npm run lint && tsc",
    "start": "node --inspect=5858 .",
    "start:prod": "cross-env NODE_ENV=prod node .",
    "clean": "rimraf ./dist/build -rf ",
    "watch": "nodemon"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CMath04/base-node-typeorm-routing.git"
  },
  "author": "Mathieu Chaussier",
  "bugs": {
    "url": "https://github.com/CMath04/base-node-typeorm-routing/issues"
  },
  "homepage": "https://github.com/CMath04/base-node-typeorm-routing#readme",
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "exec": "npm run build && npm start",
    "ext": "ts",
    "delay": 1000
  },
  "jest": {
    "preset": "ts-jest",
    "verbose": true,
    "testRegex": "(/tests/.*(spec|test))\\.ts?$",
    "testEnvironment": "node"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/body-parser": "^1.19.0",
    "@types/express": "^4.17.3",
    "@types/jest": "^25.2.1",
    "@types/jsonwebtoken": "^8.3.8",
    "@types/multer": "^1.4.2",
    "@types/node": "^13.9.8",
    "@types/source-map-support": "^0.5.1",
    "cross-env": "^6.0.3",
    "jest": "^25.3.0",
    "nodemon": "^2.0.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^25.3.1",
    "tslint": "^6.1.0",
    "typeorm-transactional-tests": "^1.1.4",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "bcrypt": "^4.0.1",
    "body-parser": "^1.19.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.11.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.2",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.8.0",
    "source-map-support": "^0.5.16",
    "sqlite3": "^4.1.1",
    "typedi": "^0.8.0",
    "typeorm": "^0.2.24",
    "typeorm-typedi-extensions": "^0.2.3",
    "winston": "^3.2.1"
  }
}
